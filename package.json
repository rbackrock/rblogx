{
  "name": "rblogx",
  "version": "1.0.0",
  "description": "rblogx",
  "scripts": {
    "dev-pc": "next ./next-project/pc -p 3000",
    "build-pc": "next build ./next-project/pc",
    "start-pc": "next start ./next-project/pc -p 3000",
    "dev-mobile": "next ./next-project/mobile -p 3001",
    "build-mobile": "next build ./next-project/mobile",
    "start-mobile": "next start ./next-project/mobile -p 3001",
    "build": "npm run build-pc && npm run build-mobile",
    "deploy-pc": "pm2 start --name pc npm -- run start-pc",
    "deploy-mobile": "pm2 start --name mobile npm -- run start-mobile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rbackrock/rblogx.git"
  },
  "keywords": [
    "Static blog",
    "Next.js"
  ],
  "author": "yhuan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rbackrock/rblogx/issues"
  },
  "homepage": "https://github.com/rbackrock/rblogx#readme",
  "dependencies": {
    "better-scroll": "^1.15.2",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.19",
    "next": "^9.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "lost": "^8.3.1",
    "moment": "^2.27.0",
    "postcss-nested": "^4.2.3",
    "postcss-write-svg": "^3.0.1",
    "react-icons": "^3.10.0",
    "react-katex": "^2.0.2",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^10.1.2",
    "remark-math": "^2.0.1",
    "styled-jsx-plugin-postcss": "^2.0.1"
  }
}
